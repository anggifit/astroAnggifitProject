---
import BaseLayout from "../layouts/BaseLayout.astro";
import MainHeader from "../components/Homepage/MainHeader.astro";
import AboutMe from "../components/Homepage/AboutMe.astro";
import Services from "../components/Homepage/Services.astro";
import Testimonies from "../components/Homepage/Testimonies.astro";
import Prices from "../components/Homepage/Prices.astro";
import Faq from "../components/Homepage/Faq.astro";

const pageTitle = "Inicio";
const customButton = "Agenda tu Cita";
---

<BaseLayout pageTitle={pageTitle}>
  <MainHeader customButton={customButton} />
  <AboutMe sectionTitle="Sobre mi" />
  <Services />
  <Testimonies />
  <Prices />
  <Faq /> 
</BaseLayout>
